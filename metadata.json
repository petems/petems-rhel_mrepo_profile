{
  "name": "petems-rhel_mrepo_profiles",
  "version": "0.1.11",
  "author": "petems",
  "summary": "Profiles for mirroring RHEL repos with mrepo",
  "license": "Apache-2.0",
  "source": "https://github.com/petems/petems-rhel_mrepo_profiles",
  "project_page": "https://github.com/petems/petems-rhel_mrepo_profiles",
  "issues_url": "https://github.com/petems/petems-rhel_mrepo_profiles/issues",
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 1.0.0"
    },
    {
      "name": "puppetlabs-mrepo",
      "version_requirement": ">= 1.0.0 <= 2.0.0"
    },
    {
      "name": "puppetlabs-git",
      "version_requirement": ">= 0.4.0 <= 1.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "6",
        "7"
      ]
    }
  ]
}
